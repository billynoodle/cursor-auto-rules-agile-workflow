{
  "name": "cursor-auto-rules-agile-workflow",
  "version": "1.0.0",
  "description": "Agile workflow project with consolidated testing",
  "main": "index.js",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "test": "jest --config tests/jest.config.ts",
    "build": "npm run build --workspace=client",
    "dev": "npm run dev --workspace=client",
    "test:unit": "jest --config tests/jest.config.ts --testPathPattern=tests/unit --coverage",
    "test:integration": "jest --config tests/jest.config.ts --testPathPattern=tests/integration --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:coverage": "jest --config tests/jest.config.ts --coverage",
    "test:tooltip": "node scripts/run-tooltip-tests.js",
    "test:cleanup": "node tests/scripts/cleanup-test-artifacts.js",
    "pretest": "npm run test:cleanup",
    "test:unit-review": "node tests/scripts/run-unit-tests.js",
    "tooltip-trends": "node scripts/view-tooltip-trends.js",
    "test:watch": "jest --config tests/jest.config.ts --watch",
    "fix-imports": "ts-node --project scripts/tsconfig.json scripts/fix-imports.ts"
  },
  "config": {
    "force": false
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@playwright/test": "^1.42.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/glob": "8.1.0",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.5",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@types/testing-library__jest-dom": "5.14.9",
    "babel-jest": "^29.7.0",
    "date-fns": "^3.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-watch-typeahead": "^2.2.2",
    "jsdom": "^26.0.0",
    "ts-jest": "^29.2.6",
    "typescript": "^5.7.3",
    "glob": "^10.3.10",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.1",
    "@types/uuid": "^10.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^11.1.0"
  }
}
